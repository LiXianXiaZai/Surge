[Script]
权限修改1 = type=http-response,pattern=(?i)\/emby\/Users\/\w{32}\/Items\/\d+,requires-body=true,max-size=0,script-path=https://raw.githubusercontent.com/LiXianXiaZai/Surge/main/SenPlayer/Download.js,debug=false
权限修改2 = type=http-response,pattern=(?i)\/emby\/Shows\/\d+\/Episodes,requires-body=true,max-size=0,script-path=https://raw.githubusercontent.com/LiXianXiaZai/Surge/main/SenPlayer/Download.js,debug=false
播放信息UA修改 = type=http-request,pattern=(?i)\/emby\/Items\/\d+/PlaybackInfo,requires-body=false,script-path=https://raw.githubusercontent.com/LiXianXiaZai/Surge/main/SenPlayer/Download.js,debug=false
下载地址UA修改 = type=http-request,pattern=(?i)\/emby\/Videos\/\d+/.+,requires-body=false,script-path=https://raw.githubusercontent.com/LiXianXiaZai/Surge/main/SenPlayer/Download.js,debug=false
